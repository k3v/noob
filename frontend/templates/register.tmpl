<html>
    <head>
        <title>Register</title>
    </head>
    <body>
{{if .Message}}
        <p>Alert: {{.Message}}</p>
{{end}}
        <form method="POST" action="/api/auth/register">
            <input type="text" name="username">
            <br>
            <input type="password" name="password">
            <br>
            <input type="submit" value="Register">
        </form>
    </body>
</html>
